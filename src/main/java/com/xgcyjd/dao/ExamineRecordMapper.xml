<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xgcyjd.dao.ExamineRecordMapper">
    <insert id="insertExamineRecord" parameterType="com.xgcyjd.po.ExamineRecord">
         insert into examine_record values
            (#{id},
            #{time},
            #{examiner_stu_id},
            #{examined_stu_id},
            #{system_id},
            #{detail},
            #{grade},
            #{remark},
            #{state})

    </insert>
</mapper>